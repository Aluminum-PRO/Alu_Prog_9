<Page x:Class="Alu_Prog_9.Pages.Store_Pages.Settings.Store_Settings_Data_Loaded_Page"
      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
      xmlns:local="clr-namespace:Alu_Prog_9.Pages.Store_Pages.Settings"
      mc:Ignorable="d" 
      d:DesignHeight="620" d:DesignWidth="770"
      Title="Store_Settings_Data_Loaded_Page">

    <Grid>
        <StackPanel>
            <StackPanel Orientation="Horizontal" Margin="10">
                <CheckBox x:Name="Load_Soft_Info_But" Style="{StaticResource MaterialDesignSwitchLightToggleButton}" Click="Load_Soft_Info_But_Click"/>
                <TextBlock Text=" Подгружать информацию о стороннем софте при запуске Al-Store" FontSize="20" TextAlignment="Left" Foreground="White"/>
            </StackPanel>
        </StackPanel>
    </Grid>
</Page>
